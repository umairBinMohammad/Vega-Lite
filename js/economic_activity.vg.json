{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 400,
    "title": {
      "text": "Victorian Economic Activity Over Time",
      "fontSize": 16,
      "fontWeight": "bold",
      "anchor": "middle"
    },
    "data": {
      "url": "data/VictorianEconomicActivity.csv",
      "format": {"type": "csv"}
    },
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Date",
        "type": "temporal",
        "title": "Date"
      },
      "y": {
        "field": "SFD",
        "type": "quantitative",
        "title": "SFD"
      },
      "color": {
        "value": "blue"
      },
      "tooltip": [
        {"field": "Date", "type": "temporal", "title": "Date"},
        {"field": "SFD", "type": "quantitative", "title": "SFD"}
      ]
    }
  }